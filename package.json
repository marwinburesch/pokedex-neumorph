{
  "name": "pokedex-neumorph",
  "version": "1.0.0",
  "description": "Pokedex neumorphism style",
  "private": true,
  "scripts": {
    "build": "npx webpack",
    "test": "eslint src/**",
    "start": "webpack-dev-server --open"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/marwinburesch/pokedex-neumorph.git"
  },
  "keywords": [
    "pokemon",
    "pokedex",
    "webpack",
    "neumorphism",
    "ui"
  ],
  "author": "Marwin Buresch",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/marwinburesch/pokedex-neumorph/issues"
  },
  "homepage": "https://github.com/marwinburesch/pokedex-neumorph#readme",
  "devDependencies": {
    "clean-webpack-plugin": "^3.0.0",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.10.0",
    "eslint-plugin-prettier": "^3.1.2",
    "html-webpack-plugin": "^3.2.0",
    "husky": "^4.2.3",
    "lint-staged": "^10.0.8",
    "prettier": "^1.19.1",
    "webpack": "^4.42.0",
    "webpack-cli": "^3.3.11",
    "webpack-dev-server": "^3.10.3"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "pre-push": "npm test"
    }
  },
  "lint-staged": {
    "*.{js}": "eslint --fix",
    "*.{js,css,scss,md,json}": "prettier --wirte"
  }
}
